class Solution {
public:
    bool isPalindrome(int n ){
        int num1;
        long long num2 =0;
        int num3 =n;
        while(n>0){
            num1 = n%10;
            num2 = (num2*10)+num1;
            n=n/10;
        }
        if(num2==num3) return true;
        else return false;
        
        
    }
};
